{
    "name": "vue-slide-unlock",
    "version": "0.3.4",
    "description": "Vue.js slide to unlock component. Protect users from accidental clicks or protect your web app from bot attack.",
    "author": {
        "name": "Anton Zolotov",
        "email": "antonzolotov@outlook.com"
    },
    "type": "module",
    "files": [
        "dist"
    ],
    "main": "dist/vue-slide-unlock.umd.js",
    "module": "dist/vue-slide-unlock.es.js",
    "types": "dist/types/index.d.ts",
    "scripts": {
        "dev": "vite",
        "build": "vite build && yarn build:types",
        "build:types": "vue-tsc --project tsconfig.types.json --declaration --emitDeclarationOnly --outDir dist/types ",
        "typecheck": "vue-tsc --project tsconfig.build-types.json --noEmit",
        "lint": "npm run lint:js && npm run lint:style",
        "build-demo": "vue-cli-service build --prefix docs --dest docs",
        "lint:js": "eslint --fix --ext .js,.vue --ignore-path .eslintignore .",
        "lint:style": "stylelint **/*.{vue,css,scss} --ignore-path .gitignore --fix --config ./.stylelintrc-format",
        "test": "echo \"Error: no test specified\" && exit 1"
    },
    "dependencies": {
        "easy-ease": "^1.0.9",
        "lodash.debounce": "^4.0.8"
    },
    "devDependencies": {
        "@babel/types": "^7.20.7",
        "@types/lodash.debounce": "^4.0.7",
        "@types/node": "^18.14.0",
        "@vitejs/plugin-vue": "^4.0.0",
        "@vue/tsconfig": "^0.1.3",
        "eslint": "^8.34.0",
        "eslint-plugin-import": "^2.27.5",
        "eslint-plugin-node": "^11.1.0",
        "eslint-plugin-promise": "^6.1.1",
        "eslint-plugin-standard": "^5.0.0",
        "eslint-plugin-vue": "^9.9.0",
        "lint-staged": "^13.1.2",
        "postcss": "^8.4.21",
        "postcss-html": "^1.5.0",
        "sass": "^1.58.3",
        "stylelint": "^15.2.0",
        "stylelint-config-property-sort-order-smacss": "^9.0.0",
        "stylelint-config-standard-scss": "^7.0.1",
        "stylelint-order": "^6.0.2",
        "stylelint-scss": "^4.4.0",
        "ts-node": "^10.9.1",
        "typescript": "^4.9.5",
        "vite": "^4.1.3",
        "vite-plugin-style-inject": "^0.0.1",
        "vue": "^3.2.47",
        "vue-tsc": "^1.1.4"
    },
    "bugs": {
        "url": "https://github.com/joseph2/vue-slide-unlock/issues"
    },
    "homepage": "https://joseph2.github.io/vue-slide-unlock/",
    "keywords": [
        "vue",
        "slide",
        "swipe",
        "unlock"
    ],
    "license": "MIT",
    "packageManager": "yarn@3.4.1",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/joseph2/vue-slide-unlock.git"
    }
}