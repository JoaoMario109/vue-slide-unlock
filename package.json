{
    "name": "vue-slide-unlock",
    "version": "0.4.9",
    "description": "Vue.js slide to unlock component. Protect users from accidental clicks or protect your web app from bot attack.",
    "author": {
        "name": "Anton Zolotov",
        "email": "antonzolotov@outlook.com"
    },
    "type": "module",
    "files": [
        "dist"
    ],
    "main": "dist/vue-slide-unlock.umd.js",
    "module": "dist/vue-slide-unlock.es.js",
    "types": "dist/types/index.d.ts",
    "workspaces": [
        "demo"
    ],
    "scripts": {
        "dev": "vite",
        "build": "vite build && yarn build:types",
        "build:types": "vue-tsc --project tsconfig.types.json --declaration --emitDeclarationOnly --outDir dist/types ",
        "typecheck": "vue-tsc --project tsconfig.types.json --noEmit",
        "demo": "yarn workspace demo generate",
        "lint": "npm run lint:js && npm run lint:style",
        "lint:js": "eslint --fix --ext .js,.vue --ignore-path .eslintignore .",
        "lint:style": "stylelint **/*.{vue,css,scss} --ignore-path .gitignore --fix --config ./.stylelintrc-format",
        "test": "vitest --run",
        "test:e2e": "cypress run --component",
        "make-badges": "istanbul-badges-readme"
    },
    "nyc": {
        "reporter": [
            "html",
            "json-summary"
        ],
        "extension": [
            ".js",
            ".ts",
            ".vue"
        ]
    },
    "dependencies": {
        "easy-ease": "^1.0.9",
        "lodash.debounce": "^4.0.8"
    },
    "devDependencies": {
        "@babel/types": "^7.23.6",
        "@cypress/code-coverage": "^3.12.18",
        "@rushstack/eslint-patch": "^1.7.0",
        "@types/lodash.debounce": "^4.0.9",
        "@types/node": "^20.11.5",
        "@vitejs/plugin-vue": "^5.0.3",
        "@vitest/coverage-istanbul": "^1.2.1",
        "@vue/eslint-config-standard": "^8.0.1",
        "@vue/eslint-config-typescript": "^12.0.0",
        "@vue/test-utils": "^2.4.3",
        "@vue/tsconfig": "^0.5.1",
        "cypress": "13.6.3",
        "cypress-real-events": "^1.11.0",
        "eslint": "^8.56.0",
        "eslint-plugin-import": "^2.29.1",
        "eslint-plugin-node": "^11.1.0",
        "eslint-plugin-promise": "^6.1.1",
        "eslint-plugin-standard": "^5.0.0",
        "eslint-plugin-vue": "^9.20.1",
        "istanbul-badges-readme": "^1.8.5",
        "istanbul-lib-coverage": "^3.2.2",
        "jsdom": "^23.2.0",
        "lint-staged": "^15.2.0",
        "nyc": "^15.1.0",
        "postcss": "^8.4.33",
        "postcss-html": "^1.6.0",
        "sass": "^1.70.0",
        "stylelint": "^16.2.0",
        "stylelint-config-html": "^1.1.0",
        "stylelint-config-property-sort-order-smacss": "^10.0.0",
        "stylelint-config-recommended-vue": "^1.5.0",
        "stylelint-config-standard-scss": "^13.0.0",
        "stylelint-order": "^6.0.4",
        "stylelint-scss": "^6.0.0",
        "ts-node": "^10.9.2",
        "typescript": "5.3.3",
        "vite": "^5.0.11",
        "vite-plugin-istanbul": "^5.0.0",
        "vite-plugin-style-inject": "^0.0.1",
        "vitest": "^1.2.1",
        "vue": "^3.4.15",
        "vue-tsc": "^1.8.27"
    },
    "bugs": {
        "url": "https://github.com/joseph2/vue-slide-unlock/issues"
    },
    "homepage": "https://joseph2.github.io/vue-slide-unlock/",
    "keywords": [
        "vue",
        "slide",
        "swipe",
        "unlock"
    ],
    "license": "MIT",
    "packageManager": "yarn@4.0.2",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/joseph2/vue-slide-unlock.git"
    }
}